{
  "name": "vue-simple-admin",
  "version": "0.0.1",
  "description": "A simple vue admin template with Element UI & axios & iconfont",
  "license": "MIT",
  "scripts": {
    "dev": "vue-cli-service serve --copy",
    "serve": "vue-cli-service serve --copy",
    "build:prod": "vue-cli-service build",
    "build:stage": "vue-cli-service build --mode staging",
    "preview": "node build/index.js --preview",
    "lint": "eslint --ext .js,.vue src",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "g:tree": "",
    "gc": "git add -A && git cz && git pull && git push",
    "gm": "git cz"
  },
  "dependencies": {
    "axios": "0.19.2",
    "element-ui": "2.13.2",
    "js-cookie": "2.2.1",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "path-to-regexp": "6.1.0",
    "vue": "2.6.11",
    "vue-router": "3.2.0",
    "vuex": "3.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/register": "7.9.0",
    "@vue/cli-plugin-babel": "4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-plugin-unit-jest": "4.3.1",
    "@vue/cli-service": "4.3.1",
    "@vue/test-utils": "1.0.3",
    "@vuepress/plugin-active-header-links": "^1.5.0",
    "@vuepress/plugin-nprogress": "^1.5.0",
    "autoprefixer": "^9.8.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.0.1",
    "chalk": "4.0.0",
    "connect": "3.7.0",
    "cz-conventional-changelog": "3.2.0",
    "eslint": "7.1.0",
    "eslint-plugin-vue": "6.2.2",
    "html-webpack-plugin": "4.3.0",
    "runjs": "^4.4.2",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "script-ext-html-webpack-plugin": "2.1.4",
    "script-loader": "0.7.2",
    "serve-static": "^1.14.1",
    "svg-sprite-loader": "5.0.0",
    "svgo": "1.3.2",
    "vue-template-compiler": "2.6.11"
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
